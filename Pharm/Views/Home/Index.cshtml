@using DataLayer
@model  List<PharmacySPResult>
@*@{ Layout = "_Layout"; }*@

<header>
    <partial name="_BarAll" />
</header>

<body>
    <table>
        @foreach (var ph in Model)
        {
        <tr>
            <td>@ph.PhName</td>
            <td>@ph.StateCode</td>
            <td>@ph.Address</td>
            <td>@ph.ContactEmail</td>
            <td>@ph.ContactPhone </td>

        </tr>
        }
    </table>
    <partial name="_ViewAdvertPartial" />
</body>
